{
    "name": "ziffdavis/laravel-cloudflare",
    "description": "Laravel helpers for managing cache tagging and purging in Cloudflare (Enterprise)",
    "type": "library",
    "require": {
        "php": "^7.1",
        "cloudflare/sdk": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "laravel/laravel": "^5.6.0",
        "orchestra/testbench": "^3.8"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ZiffDavis\\Laravel\\Cloudflare\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jeff Carouth",
            "email": "jeff.carouth@ziffmedia.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "ZiffDavis\\Laravel\\Cloudflare\\Providers\\CloudflareServiceProvider"
            ]
        }
    }
}
